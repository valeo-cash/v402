{
  "name": "@v402pay/gateway",
  "version": "0.1.0",
  "description": "v402pay gateway middleware for Express, Fastify, and Next.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@v402pay/core": "workspace:*",
    "@supabase/supabase-js": "^2.47.0"
  },
  "devDependencies": {
    "@noble/ed25519": "^2.1.0",
    "@solana/web3.js": "^1.95.0",
    "@types/express": "^4.17.21",
    "@v402pay/sdk": "workspace:*",
    "fastify": "^4.28.0",
    "tsup": "^8.3.5",
    "typescript": "~5.6.0",
    "vitest": "^2.1.6"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "fastify": "^4.0.0",
    "next": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "fastify": { "optional": true },
    "next": { "optional": true }
  }
}
