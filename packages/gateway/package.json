{
  "name": "@v402pay/gateway",
  "version": "0.3.0",
  "description": "v402pay gateway middleware for Express, Fastify, and Next.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.mjs",
      "require": "./dist/next.js"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "license": "MIT",
  "author": "Valeo",
  "homepage": "https://github.com/valeo-cash/v402#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/valeo-cash/v402.git",
    "directory": "packages/gateway"
  },
  "keywords": ["v402", "payment", "solana", "usdc", "402", "web3", "gateway", "middleware", "express", "fastify", "nextjs"],
  "engines": { "node": ">=18" },
  "publishConfig": { "access": "public" },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@v402pay/core": "workspace:0.3.0",
    "@supabase/supabase-js": "^2.47.0"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "fastify": "^4.0.0",
    "next": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "fastify": { "optional": true },
    "next": { "optional": true }
  },
  "devDependencies": {
    "@noble/ed25519": "^2.1.0",
    "@solana/web3.js": "^1.95.0",
    "@types/express": "^4.17.21",
    "@v402pay/sdk": "workspace:0.3.0",
    "fastify": "^4.28.0",
    "tsup": "^8.3.5",
    "typescript": "~5.6.0",
    "vitest": "^2.1.6"
  }
}
